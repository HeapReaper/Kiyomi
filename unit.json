{
  "listeners": {
    "*:8000": {
      "pass": "routes",
      "forwarded": {
        "protocol": "X-Forwarded-Proto",
        "source": ["78.46.134.87"]
      }
    }
  },
  "routes": [
    {
      "match": {
        "uri": "!/index.php"
      },
      "action": {
        "share": "/var/www/html/public$uri",
        "fallback": {
          "pass": "applications/laravel"
        }
      }
    }
  ],
  "applications": {
    "laravel": {
      "type": "php",
      "root": "/var/www/html/public",
      "script": "index.php"
    }
  }
}
