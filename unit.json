{
  "listeners": {
    "*:8000": {
      "pass": "routes",
      "forwarded": {
        "protocol": "X-Forwarded-Proto",
        "source": [
          "172.17.0.0/16",
          "172.18.0.0/16",
          "172.19.0.0/16",
          "172.20.0.0/16",
          "172.21.0.0/16",
          "172.22.0.0/16",
          "172.19.0.0/16"
        ]
      }
    }
  },
  "routes": [
    {
      "match": {
        "uri": "!/index.php"
      },
      "action": {
        "share": "/var/www/html/public$uri",
        "fallback": {
          "pass": "applications/laravel"
        }
      }
    }
  ],
  "applications": {
    "laravel": {
      "type": "php",
      "root": "/var/www/html/public",
      "script": "index.php"
    }
  }
}
